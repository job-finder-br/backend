    "dev:seed:run": "NODE_ENV=dev tsnd -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "start": "node src/infra/http/server.js",
    "typeorm": "NODE_ENV=production ./node_modules/typeorm/cli.js",
    "seed:run": "NODE_ENV=production ./node_modules/typeorm-seeding/dist/cli.js seed",
